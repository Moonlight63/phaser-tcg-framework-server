{
  "environmental": {
    "secret": "your_secret_key_here",
    "listenOn": "3000",
    "basePath": "/api",
    "version": "1.0"
  },
  "userStorage": {
    "type": "FileStore",
    "filePath": "./users.json",
    "timeout": "1h"
  },
  "sessionStorage": {
    "type": "FileStore",
    "timeout": "30m"
  },
  "lobbyStorage": {
    "type": "InMemory",
    "abandonedTime": "15m"
  }
}
